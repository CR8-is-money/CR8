{
  "name": "cr8",
  "version": "0.1.0-alpha",
  "description": "Pack up your DAI",
  "keywords": [
    "blockchain",
    "ethereum",
    "dai",
    "decentralized",
    "ether",
    "defi",
    "decentralized finance"
  ],
  "license": "AGPL-3.0-only",
  "main": "src/index.js",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test": "truffle test",
    "testjs": "jest src/",
    "lint": "solium -d contracts/ && eslint test/ migrations/ src/",
    "compile": "truffle compile",
    "coverage": "truffle run coverage",
    "lint-vue": "vue-cli-service lint",
    "lint:fix": "solium --fix -d contracts/ && eslint --fix test migrations/ src/",
    "deploy:app": "npm run build && gh-pages -d dist",
    "deploy:contracts": "truffle migrate --network kovan --reset --skip-dry-run -f 2"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "bn.js": "^5.1.1",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.2",
    "gsap": "^3.2.2",
    "material-design-icons-iconfont": "^5.0.1",
    "openzeppelin-solidity": "^2.5.0",
    "truffle-hdwallet-provider": "^1.0.17",
    "truffle-plugin-verify": "^0.3.9",
    "vue": "^3.0.0",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^6.1.2",
    "gh-pages": "^3.0.0",
    "jest": "^25.1.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "solidity-coverage": "^0.7.2",
    "solium": "^1.2.5",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "engines": {
    "node": ">=11.12.0"
  }
}
